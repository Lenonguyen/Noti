  "bugs": {
    "url": "https://github.com/longro3000/Noti/issues"
  },
  "homepage": "https://github.com/longro3000/Noti#readme",
  "dependencies": {
    "concurrent": "^0.3.2",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "mongoose": "^5.3.15",
    "nodemon": "^1.18.7",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  }
}
